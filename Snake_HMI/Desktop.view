        <script data-tchmi-target-attribute="data-tchmi-trigger" type="application/json">
[
  {
    "event": "%s%PLC1.GVL_Variables.bDraw|Interval=2%/s%",
    "actions": [
      {
        "objectType": "Function",
        "active": true,
        "fn": "TcHmi.Functions.Snake_HMI.DrawFood",
        "fnParams": [
          {
            "objectType": "Symbol",
            "valueType": "tchmi:server#/definitions/PLC1.ST_Food",
            "symbolExpression": "%s%PLC1.GVL_Variables.stFood%/s%"
          }
        ],
        "asyncWait": true
      },
      {
        "objectType": "Function",
        "active": true,
        "fn": "TcHmi.Functions.Snake_HMI.DrawSnake",
        "fnParams": [
          {
            "objectType": "Symbol",
            "valueType": "tchmi:server#/definitions/PLC1.ARRAY (1..10) OF ST_SnakePart",
            "symbolExpression": "%s%PLC1.GVL_Variables.aStSnakeBody|Interval=2%/s%"
          },
          {
            "objectType": "Symbol",
            "valueType": "tchmi:general#/definitions/INT",
            "symbolExpression": "%s%PLC1.GVL_Variables.iSnakeLength%/s%"
          },
          {
            "objectType": "Symbol",
            "valueType": "tchmi:general#/definitions/INT",
            "symbolExpression": "%s%PLC1.GVL_Constants.SNAKE_LENGTH%/s%"
          }
        ],
        "asyncWait": true
      }
    ]
  },
  {
    "event": "%s%PLC1.GVL_Variables.bGameOver%/s%",
    "actions": [
      {
        "objectType": "Condition",
        "active": true,
        "parts": [
          {
            "if": [
              {
                "compare1": {
                  "objectType": "Symbol",
                  "valueType": "tchmi:general#/definitions/BOOL",
                  "symbolExpression": "%s%PLC1.GVL_Variables.bGameOver%/s%"
                },
                "compare2": {
                  "objectType": "StaticValue",
                  "valueType": "tchmi:general#/definitions/BOOL",
                  "value": true
                },
                "compareOperator": "==",
                "logic": null
              }
            ],
            "then": [
              {
                "objectType": "JavaScript",
                "active": true,
                "sourceLines": [
                  "swal({",
                  "  title: \"Game Over!\",",
                  "  text: \"Do you want to play again?\",",
                  "  icon: \"warning\",",
                  "  buttons: true,",
                  "  dangerMode: true,",
                  "})",
                  ".then((willDelete) => {",
                  "  if (willDelete) {",
                  "\tTcHmi.Symbol.writeEx(\"%s%PLC1.GVL_Variables.bInit%/s%\", 1); ",
                  "\twindow.location.reload();\t",
                  "    swal(\"Game Started!\", {",
                  "      icon: \"success\",",
                  "    });",
                  "  } else {",
                  "\tTcHmi.Symbol.writeEx(\"%s%PLC1.GVL_Variables.bInit%/s%\", 0);   ",
                  "    swal(\"Game Ended!\");",
                  "  }",
                  "});"
                ],
                "injectContextObject": false,
                "asyncWait": true,
                "waitMode": "Synchronous"
              },
              {
                "objectType": "WriteToSymbol",
                "active": true,
                "symbolExpression": "%s%PLC1.GVL_Variables.bInit%/s%",
                "value": {
                  "objectType": "StaticValue",
                  "valueType": "tchmi:general#/definitions/BOOL",
                  "value": false
                },
                "asyncWait": true
              }
            ]
          },
          {
            "else": []
          }
        ],
        "asyncWait": true
      }
    ]
  }
]
</script>
